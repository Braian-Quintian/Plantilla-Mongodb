{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "nodemon --quiet ./backend/server/server.js",
    "install": "npm i -E express express-validator express-rate-limit express-routes-versioning jose mongodb passport passport-http-bearer bcrypt",
    "install-dev": "npm i -D nodemon @types/express @types/passport-http-bearer @types/express-routes-versioning",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "bcrypt": "5.1.1",
    "cors": "2.8.5",
    "express": "4.18.2",
    "express-rate-limit": "7.1.3",
    "express-routes-versioning": "1.0.1",
    "express-validator": "7.0.1",
    "jose": "5.0.2",
    "mongodb": "6.2.0",
    "passport": "0.6.0",
    "passport-http-bearer": "1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/express-routes-versioning": "^1.0.3",
    "@types/passport-http-bearer": "^1.0.39",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "nodemon": "^3.0.1",
    "vite": "^4.4.5"
  }
}
